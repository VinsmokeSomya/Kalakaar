{
  "framework": "sveltekit",
  "buildCommand": "cd kalakaar && npm run build",
  "installCommand": "cd kalakaar && npm install",
  "outputDirectory": "kalakaar/.vercel/output",
  "devCommand": "cd kalakaar && npm run dev",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ],
  "env": {
    "GEMINI_API_KEY": "${GEMINI_API_KEY}"
  }
} 